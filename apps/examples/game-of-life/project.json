{
  "name": "game-of-life",
  "sourceRoot": "apps/examples/game-of-life/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "game-of-life:build",
        "inspect": "inspect",
        "port": 33348
      }
    },
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "compiler": "tsc",
        "generatePackageJson": true,
        "main": "apps/examples/game-of-life/src/entrypoints/main.ts",
        "outputPath": "dist/apps/examples/game-of-life",
        "target": "node",
        "tsConfig": "apps/examples/game-of-life/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": false,
          "inspect": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/examples/game-of-life/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "tools/scripts/compile.sh game-of-life examples",
            "description": "Canned script"
          }
        ]
      }
    }
  },
  "tags": ["example-app", "tty", "rgb-matrix"]
}
