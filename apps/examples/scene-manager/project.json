{
  "name": "scene-manager",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/examples/scene-manager/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "scene-manager:build",
        "inspect": "inspect",
        "port": 33344
      }
    },
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/examples/scene-manager",
        "generatePackageJson": true,
        "fileReplacements": [
          {
            "replace": "apps/examples/scene-manager/src/environments/environment.ts",
            "with": "apps/examples/scene-manager/src/environments/develop.environment.ts"
          }
        ],
        "main": "apps/examples/scene-manager/src/entrypoints/main.ts",
        "tsConfig": "apps/examples/scene-manager/tsconfig.app.json",
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": false,
          "inspect": false,
          "fileReplacements": []
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/examples/scene-manager/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
