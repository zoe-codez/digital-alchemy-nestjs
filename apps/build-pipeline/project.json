{
  "name": "build-pipeline",
  "sourceRoot": "apps/build-pipeline/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "build-pipeline:build",
        "inspect": "inspect",
        "port": 33340
      }
    },
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "assets": [],
        "outputPath": "dist/apps/build-pipeline",
        "generatePackageJson": true,
        "main": "apps/build-pipeline/src/main.ts",
        "tsConfig": "apps/build-pipeline/tsconfig.app.json",
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": false,
          "inspect": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/build-pipeline/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "tools/scripts/publish-app.sh build-pipeline",
            "description": "Canned script"
          }
        ]
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "tools/scripts/compile.sh build-pipeline",
            "description": "Canned script"
          }
        ]
      }
    }
  },
  "tags": ["terminal"]
}
