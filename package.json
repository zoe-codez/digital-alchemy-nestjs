    "hass:prep": "./scripts/hass-prep.sh",
    "hass:reset": "./scripts/hass-reset.sh",
    "container:hass:logs": "docker compose -f ./docker/homeassistant/docker-compose.yaml logs -f --tail 20",
    "container:hass:restart": "docker compose -f ./docker/homeassistant/docker-compose.yaml restart",
    "container:hass:start": "docker compose -f ./docker/homeassistant/docker-compose.yaml up -d",
    "container:hass:stop": "docker compose -f ./docker/homeassistant/docker-compose.yaml stop",
    "container:redis:start": "docker compose -f ./docker/redis/docker-compose.yaml up -d",
    "container:redis:stop": "docker compose -f ./docker/redis/docker-compose.yaml stop"
  },
