    "record": "terminalizer record -c ./terminalizer.yml -k",
    "play": "terminalizer play",
    "render": "terminalizer render",
    "hass:compress": "./scripts/compress.sh",
    "hass:cleanup": "rm -rf ./docker/homeassistant/config",
